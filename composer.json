{
  "name": "pschocke/slack-notification-settings-handler",
  "description": "Send Slack Notifications via the laravel notificationSettings package",
  "keywords": [
    "Laravel",
    "Laravel Notifications",
    "Slack"
  ],
  "homepage": "https://github.com/pschocke/laravel-notification-settings",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Schocke",
      "email": "patrick@ausbildung-ms.de",
      "homepage": "https://github.com/pschocke",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/support": "~6|~7",
    "pschocke/laravel-notification-settings": "dev-master"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "pschocke\\SlackNotificationSettings\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pschocke\\SlackNotificationSettings\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
